{
  "name": "fastbar",
  "version": "1.0.0",
  "description": "proyecto integrador, servicio de venta de tragos",
  "main": "src/index.js",
  "scripts": {
    "test": "clear && nodemon src/index.js -e json,js,mjs,ejs",
    "start": "node src/index.js",
    "sq-i": "sequelize init",
    "sq-ms": "sequelize init:migrations && sequelize init:seeders",
    "sq-mg": "sequelize migration:generate --name users",
    "sq-sg": "sequelize seed:generate --name categories",
    "sq-db": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "method-override": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1"
  }
}
